{
  "name": "youniqueindia",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:server": "ts-node ./src/index.ts",
    "dev:worker": "ts-node ./src/index-worker.ts",
    "dev": "concurrently npm:dev:*",
    "build": "tsc",
    "start:server": "node ./dist/index.js",
    "start:worker": "node ./dist/index-worker.js",
    "start": "concurrently npm:start:*",

    "setup": "ts-node ./src/scripts/1-setup-vendure.ts",
  "seed": "ts-node ./src/scripts/import-seed-products.ts",
    "seed:local": "ts-node ./src/scripts/2-seed-products.ts",

    "db:wipe": "ts-node ./src/scripts/wipe-db.ts",
    "reset:store": "npm run db:wipe && npm run setup",
    "setup:complete": "npm run db:wipe && npm run setup && npm run seed",

    "import:products": "ts-node ./src/scripts/import-seed-products.ts",

    "clean": "node -e \"require('fs').rmSync('./dist',{recursive:true,force:true});console.log('dist cleared');\"",
    "email:test": "ts-node ./src/scripts/send-test-email.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.864.0",
    "@pinelab/vendure-plugin-facet-suggestions": "^1.3.0",
    "@pinelab/vendure-plugin-multiserver-db-sessioncache": "^1.4.0",
    "@types/nodemailer": "^6.4.17",
    "@vendure/admin-ui-plugin": "3.3.4",
    "@vendure/asset-server-plugin": "^3.3.4",
    "@vendure/core": "3.3.4",
    "@vendure/elasticsearch-plugin": "^3.3.7",
    "@vendure/email-plugin": "^3.4.0",
    "@vendure/graphiql-plugin": "3.3.4",
    "@vendure/stellate-plugin": "^2.3.3",
    "apollo-server-core": "^3.13.0",
    "cloudinary": "^2.7.0",
    "concurrently": "9.1.2",
    "dotenv": "16.5.0",
    "graphql-tag": "^2.12.6",
    "nodemailer": "^7.0.5",
    "pg": "8.16.2",
    "prompt-sync": "^4.2.0",
  "resend": "^6.0.1",
  "razorpay": "^2.9.5"
  },
  "overrides": {
    "multer": "2.0.2",
    "qs": "6.14.0",
    "xml2js": "0.6.2",
    "on-headers": "1.1.0",
    "send": "1.2.0",
  "serve-static": "2.2.0",
  "form-data": "4.0.4"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/pg": "^8.15.4",
    "@vendure/cli": "3.3.4",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.1",
    "typescript": "5.8.2"
  }
}
